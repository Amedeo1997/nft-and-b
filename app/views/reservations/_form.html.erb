<div class="form-container">
  <h2 id="login-title" >Book your NFT now</h2>
  <%= simple_form_for [@product, @reservation] do |f| %>
  <%= f.input :start_date, html5: true %>
  <%= f.input :end_date, html5: true %>
    <div class="form-actions">
      <%= f.button :submit, class: "button-primary" %>
    </div>
<% end %>
