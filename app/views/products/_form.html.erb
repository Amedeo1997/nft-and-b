<%= simple_form_for(@product) do |f| %>
  <%= f.input :name %>
  <%= f.input :image %>
 <%= f.input :category, collection: ['Art', 'Entertainment', 'Movie', 'Gaming', 'Collections'], prompt: 'Choose one category' %>
  <%= f.input :price %>
   <%= f.input :description %>
  <%= f.button :submit %>
<% end %>
